{
    "output": "bin/distill-cli.phar",
    "alias": "distill-cli.phar",
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Php"
    ],
    "extract": false,
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": ["*.php"],
            "exclude": ["Tests", "tests"],
            "in": ["vendor", "src"]
        }
    ],
    "git-commit": "git-commit",
    "stub": "src/Distill/Cli/Resources/phar-stub.php",
    "web": false
}